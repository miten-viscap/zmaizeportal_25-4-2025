<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="MaizeProcurementPortal.MaizeProcurementPortal.controller.Gate_Pass" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Gate Pass Configuration" showNavButton="true" navButtonPress="onNavBack" class="GPConfig">
				<content>
					<FlexBox width="100%" height="100%" alignItems="Center" justifyContent="Center">
						<items>
							<GenericTile backgroundImage="Image/Tile4.jpg" header="Create Gate Pass" press="presCreateGp"
								class="sapUiSmallMarginBegin sapUiSmallMarginTop">
								<TileContent>
									<core:Icon src="sap-icon://visits" size="4rem"/>
								</TileContent>
							</GenericTile>
							<GenericTile backgroundImage="Image/tile5.jpg" header="Change/Display Gate Pass" press="pressCHaDelGp"
								class="sapUiSmallMarginBegin sapUiSmallMarginTop">
								<TileContent>
									<core:Icon src="sap-icon://employee-approvals" size="4rem"/>
								</TileContent>
							</GenericTile>
						</items>
					</FlexBox>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>