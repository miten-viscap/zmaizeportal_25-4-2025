<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Dialog id="MatCodeDialog" contentHeight="auto" contentWidth="50%">
		<Table items="{path: '/Mat0mSet'}" backgroundDesign="Transparent" contextualWidth="Auto" class="sapUiSmallMarginTop" growing="true">
			<headerToolbar>
				<OverflowToolbar>
					<Title text="Value Help Material Code" level="H2"/>
					<ToolbarSpacer/>
					<Button icon="sap-icon://decline" type="Transparent" press="cancel"></Button>
				</OverflowToolbar>
			</headerToolbar>
			<columns>
				<Column demandPopin="false">
					<Text text="Material Number " class="wrapcss"></Text>
				</Column>
				<!--	<Column>
					<Text text="Language" class="wrapcss"></Text>
				</Column>-->
				<Column>
					<Text text="Material Description" class="wrapcss"></Text>
				</Column>
			</columns>
			<items>
				<ColumnListItem type="Active" press="ValueHelpMatCode">
					<cells>
						<!--<Text text="{Mandt}"></Text>-->
						<Text text="{Matnr}"/>
						<!--	<Text text="{Spras}"></Text>-->
						<Text text="{Maktg}"></Text>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Dialog>
</core:FragmentDefinition>